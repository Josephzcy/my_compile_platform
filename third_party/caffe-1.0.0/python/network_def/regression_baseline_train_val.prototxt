layer {
  name: "Data1"
  type: "Data"
  top: "Data1"
  top: "data"
  phase: TRAIN
  transform_param {
    mirror: false
    crop_size: 40
    mean_value: 128
  }
  data_param {
    source: "/data/wzheng/data/car_regression_80/img_gray_80_db/"
    batch_size: 256
    backend: LMDB
  }
}
layer {
  name: "Convolution1"
  type: "Convolution"
  bottom: "Data1"
  top: "Convolution1"
  convolution_param {
    num_output: 16
    pad: 2
    kernel_size: 5
    stride: 1
    weight_filler {
      type: "xavier"
    }
  }
}
layer {
  name: "ReLU1"
  type: "ReLU"
  bottom: "Convolution1"
  top: "Convolution1"
}
layer {
  name: "Pooling1"
  type: "Pooling"
  bottom: "Convolution1"
  top: "Pooling1"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}
layer {
  name: "Convolution2"
  type: "Convolution"
  bottom: "Pooling1"
  top: "Convolution2"
  convolution_param {
    num_output: 32
    pad: 1
    kernel_size: 3
    stride: 1
    weight_filler {
      type: "xavier"
    }
  }
}
layer {
  name: "ReLU2"
  type: "ReLU"
  bottom: "Convolution2"
  top: "Convolution2"
}
layer {
  name: "Pooling2"
  type: "Pooling"
  bottom: "Convolution2"
  top: "Pooling2"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}
layer {
  name: "InnerProduct1"
  type: "InnerProduct"
  bottom: "Pooling2"
  top: "InnerProduct1"
  inner_product_param {
    num_output: 512
    weight_filler {
      type: "xavier"
    }
  }
}
layer {
  name: "ReLU3"
  type: "ReLU"
  bottom: "InnerProduct1"
  top: "InnerProduct1"
}
layer {
  name: "InnerProduct2"
  type: "InnerProduct"
  bottom: "InnerProduct1"
  top: "InnerProduct2"
  inner_product_param {
    num_output: 9
    weight_filler {
      type: "xavier"
    }
  }
}
layer {
  name: "Data2"
  type: "Data"
  top: "Data2"
  top: "label"
  phase: TRAIN
  data_param {
    source: "/data/wzheng/data/car_regression_80/label_gray_80_db/"
    batch_size: 256
    backend: LMDB
  }
}
layer {
  name: "EuclideanLoss1"
  type: "EuclideanLoss"
  bottom: "InnerProduct2"
  bottom: "Data2"
  top: "EuclideanLoss1"
}
layer {
  name: "Data3"
  type: "Data"
  top: "Data3"
  top: "label"
  phase: TEST
  data_param {
    source: "/data/wzheng/data/car_regression_80/label_gray_80_db/"
    batch_size: 256
    backend: LMDB
  }
}
layer {
  name: "Accuracy1"
  type: "Accuracy"
  bottom: "InnerProduct2"
  bottom: "Data3"
  top: "Accuracy1"
  phase: TEST
}
